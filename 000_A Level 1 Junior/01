
import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		
		Scanner sc=new Scanner(System.in);


		char name=sc.next().charAt(0);
		int a,b,c;
		int avg=0;
		a=sc.nextInt();
		b=sc.nextInt();
		c=sc.nextInt();
		avg=(a+b+c)/3;
		System.out.println(name);
		System.out.print(avg);


		
	}

}
