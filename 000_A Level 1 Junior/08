import java.util.Scanner;

public class Solution {
    
    public static void main(String[] args) {
        // Write your code here
        Scanner sc=new Scanner(System.in);
         
         int x=sc.nextInt();
         int y=sc.nextInt();
         int i=1;
         int power=1; 
         while(i<=y){
             power=power*x;
             i=i+1;
         }

         System.out.println(power);

        
    }
}