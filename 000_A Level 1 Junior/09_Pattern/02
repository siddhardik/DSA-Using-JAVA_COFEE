// A
// BC
// CDE
// DEFG

import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int i=1;
		while(i<=n){
           int j=1;
		   while(j<=i){
             System.out.print((char)('A'+i-1+j-1));
			 // 'A' + i-1  denotes from which character I have 
			 j++;
		   }
		   System.out.println();
			i++;
		}




		
	}

}
