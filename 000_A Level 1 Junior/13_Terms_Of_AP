import java.util.Scanner;
public class Main {
	
	public static void main(String[] args) {
		// Write your code here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
        int j=1;
		for(int i=1;j<=n;i++){
			int c=3*i +2;
			if(c%4!=0){
				System.out.print(c+" " );
				j++;

			}
		}

	}
}
