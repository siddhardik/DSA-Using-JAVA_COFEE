public class Solution {
	public static String reverseWordWise(String input) {
		if(input.length()==0){
			return "";
		}
		String words[]=input.split(" ");
		String str="";

		for(int i=(words.length-1);i>=0;i--){
			str+=words[i];
			str+=" ";
        	}

			// Remove Last Space
		return str.substring(0,str.length()-1);


	}
}
