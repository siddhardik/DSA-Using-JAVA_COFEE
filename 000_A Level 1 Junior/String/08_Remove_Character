// Sample Input 1:
// aabccbaa
// a
// Sample Output 1:
// bccb


public class Solution {

	public static String removeAllOccurrencesOfChar(String str, char ch) {
		// Your code goes here
		int n=str.length();
		String s="";
		if(n==0){
			return s
		}

		for(int i=0;i<n;i++){
			if(str.charAt(i)!=ch){
                 s+=str.charAt(i);
			}
		}

		return s;
	}

}